<UserControl x:Class="VisualProfilerUI.View.MethodView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Name="root" 
             MouseEnter="RectangleMouseEnter" 
             BorderBrush="{Binding Path=BorderBrush}" 
             BorderThickness="{Binding Path=BorderThinkness}" >
   
        <Rectangle Width="50" 
               Height="{Binding Path=Height, ElementName=root}" 
               Fill="{Binding Path=Fill, ElementName=root}" />
   
    <!--<Rectangle Width="50" Height="10" Fill="Red" />-->
</UserControl>
