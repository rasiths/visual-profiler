<UserControl x:Class="VisualProfilerUI.View.DetailView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Name="root"
             >
    <StackPanel>
        <TextBlock Text="{Binding Path=Metrics, ElementName=root}"  />
        <TextBlock Text="{Binding Path=MethodName, ElementName=root}"  />
    </StackPanel>
</UserControl>
